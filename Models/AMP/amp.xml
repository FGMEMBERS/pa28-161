<?xml version="1.0"?>

<PropertyList>

 <path>amp.ac</path>

 <animation>
  <type>material</type>
  <object-name>Face</object-name>
  <object-name>AmpNeedle</object-name>
  <emission>
   <red>1.0</red>
   <green>0.2</green>
   <blue>0.0</blue>
   <factor-prop>/sim/model//material/instruments/factor</factor-prop>
  </emission>
 </animation>

 <animation>
  <type>rotate</type>
  <object-name>AmpNeedle</object-name>
  <property>/systems/electrical/amps[0]</property>
  <interpolation>
   <entry><ind>  0.0</ind><dep>-38 </dep></entry>     
   <entry><ind>  20</ind><dep>-13</dep></entry>     
   <entry><ind>  40</ind><dep>13</dep></entry>     
   <entry><ind> 60</ind><dep> 38 </dep></entry>     
  </interpolation>
  <center>
   <x-m>0.0</x-m>
   <y-m>-0.00015</y-m>
   <z-m>-0.01456</z-m>
  </center>
  <axis>
   <x>-1</x>
   <y>0</y>
   <z>0</z>
  </axis>
 </animation>

</PropertyList>


