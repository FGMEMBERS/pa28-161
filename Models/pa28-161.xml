 <animation>
  <type>select</type>
  <object-name>Propeller</object-name>
  <condition>
   <less-than>
    <property>engines/engine[0]/rpm</property>
    <value>400</value>
   </less-than>
  </condition>
 </animation>

 <animation>
  <type>spin</type>
  <object-name>Propeller</object-name>
  <property>engines/engine[0]/rpm</property>
  <factor>-1</factor>
  <center>
   <x-m>0</x-m>
   <y-m>0</y-m>
   <z-m>0</z-m>
  </center>
  <axis>
   <x>1.0</x>
   <y>0.0</y>
   <z>0.0</z>
  </axis>
 </animation>

<!-- Make the fuel selector valve move based on the value of switch-position 
     The actual change in tank selection is done by pa28.nas which allows
     cycling the selected tank by keyboard "f".-->
 <animation>
  <type>rotate</type>
  <object-name>FuelValvePointer</object-name>
  <offset-deg>-180</offset-deg>
  <factor>-90</factor>
  <property>controls/fuel/switch-position</property>  
  <center>
   <x-m>1.94707</x-m>
   <y-m>-0.55448</y-m>
   <z-m>-0.21952</z-m>
  </center>
  <axis>
   <x>0.00042</x>
   <y>0.01261</y>
   <z>0.0018</z>
  </axis>
 </animation>

 <animation>
  <type>rotate</type>
  <object-name>ParkingBrakeControl</object-name>
  <factor>35</factor>
  <property>controls/gear/brake-parking</property>  
  <center>
   <x-m>1.891</x-m>
   <y-m>-0.0</y-m>
   <z-m>-0.0616</z-m>
  </center>
  <axis>
   <x>0.0</x>
   <y>-1.0</y>
   <z>0.0</z>
  </axis>
 </animation>

